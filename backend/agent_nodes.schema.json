{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Complex",
  "type": "object",
  "properties": {
    "defaults": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/$defs/DataValue"
      }
    },
    "end_node": {
      "type": "string",
      "format": "uuid"
    },
    "inputs": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/DataType"
      }
    },
    "instances": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/$defs/Instance"
      }
    },
    "outputs": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/DataType"
      }
    }
  },
  "required": [
    "inputs",
    "outputs",
    "end_node",
    "defaults",
    "instances"
  ],
  "$defs": {
    "AtomicBinOp": {
      "type": "string",
      "enum": [
        "Add",
        "Sub",
        "Mul",
        "Div",
        "Pow",
        "Mod"
      ]
    },
    "AtomicIo": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "Read",
            "Write",
            "GetLine"
          ]
        },
        {
          "type": "object",
          "properties": {
            "Open": {
              "$ref": "#/$defs/IoType"
            }
          },
          "additionalProperties": false,
          "required": [
            "Open"
          ]
        }
      ]
    },
    "AtomicLogic": {
      "type": "string",
      "enum": [
        "And",
        "Or",
        "Xor",
        "Not",
        "Eq"
      ]
    },
    "AtomicType": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "Print",
            "Replace",
            "IsNone"
          ]
        },
        {
          "type": "object",
          "properties": {
            "BinOp": {
              "$ref": "#/$defs/AtomicBinOp"
            }
          },
          "additionalProperties": false,
          "required": [
            "BinOp"
          ]
        },
        {
          "type": "object",
          "properties": {
            "UnaryOp": {
              "$ref": "#/$defs/AtomicUnaryOp"
            }
          },
          "additionalProperties": false,
          "required": [
            "UnaryOp"
          ]
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "#/$defs/DataValue"
            }
          },
          "additionalProperties": false,
          "required": [
            "Value"
          ]
        },
        {
          "type": "object",
          "properties": {
            "Control": {
              "$ref": "#/$defs/ControlFlow"
            }
          },
          "additionalProperties": false,
          "required": [
            "Control"
          ]
        },
        {
          "type": "object",
          "properties": {
            "Variable": {
              "type": "array",
              "maxItems": 3,
              "minItems": 3,
              "prefixItems": [
                {
                  "$ref": "#/$defs/DataType"
                },
                {
                  "type": "string",
                  "format": "uuid"
                },
                {
                  "type": "integer",
                  "format": "uint",
                  "minimum": 0
                }
              ]
            }
          },
          "additionalProperties": false,
          "required": [
            "Variable"
          ]
        },
        {
          "type": "object",
          "properties": {
            "Io": {
              "$ref": "#/$defs/AtomicIo"
            }
          },
          "additionalProperties": false,
          "required": [
            "Io"
          ]
        },
        {
          "type": "object",
          "properties": {
            "Cast": {
              "$ref": "#/$defs/DataType"
            }
          },
          "additionalProperties": false,
          "required": [
            "Cast"
          ]
        },
        {
          "type": "object",
          "properties": {
            "LogicalOp": {
              "$ref": "#/$defs/AtomicLogic"
            }
          },
          "additionalProperties": false,
          "required": [
            "LogicalOp"
          ]
        }
      ]
    },
    "AtomicUnaryOp": {
      "type": "string",
      "enum": [
        "Neg"
      ]
    },
    "ControlFlow": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "Start",
            "End"
          ]
        },
        {
          "type": "object",
          "properties": {
            "WaitForInit": {
              "type": "array",
              "maxItems": 3,
              "minItems": 3,
              "prefixItems": [
                {
                  "$ref": "#/$defs/DataType"
                },
                {
                  "type": "string",
                  "format": "uuid"
                },
                {
                  "type": "integer",
                  "format": "uint",
                  "minimum": 0
                }
              ]
            }
          },
          "additionalProperties": false,
          "required": [
            "WaitForInit"
          ]
        },
        {
          "type": "object",
          "properties": {
            "While": {
              "type": "array",
              "maxItems": 3,
              "minItems": 3,
              "prefixItems": [
                {
                  "$ref": "#/$defs/DataType"
                },
                {
                  "type": "string",
                  "format": "uuid"
                },
                {
                  "type": "integer",
                  "format": "uint",
                  "minimum": 0
                }
              ]
            }
          },
          "additionalProperties": false,
          "required": [
            "While"
          ]
        }
      ]
    },
    "DataType": {
      "type": "string",
      "enum": [
        "Array",
        "String",
        "Integer",
        "Float",
        "Boolean",
        "Byte",
        "Handle",
        "Object",
        "None"
      ]
    },
    "DataValue": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "integer",
          "format": "int64"
        },
        {
          "type": "number",
          "format": "double"
        },
        {
          "type": "boolean"
        },
        {
          "type": "integer",
          "format": "uint8",
          "maximum": 255,
          "minimum": 0
        },
        {
          "type": "array",
          "items": {
            "$ref": "#/$defs/DataValue"
          }
        },
        {
          "type": "string",
          "format": "uuid"
        },
        {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/DataValue"
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "Instance": {
      "type": "object",
      "properties": {
        "default_overrides": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/DataValue"
          }
        },
        "inputs": {
          "type": "array",
          "items": {
            "type": "array",
            "maxItems": 3,
            "minItems": 3,
            "prefixItems": [
              {
                "$ref": "#/$defs/DataType"
              },
              {
                "type": "string",
                "format": "uuid"
              },
              {
                "type": "integer",
                "format": "uint",
                "minimum": 0
              }
            ]
          }
        },
        "node_type": {
          "$ref": "#/$defs/NodeType"
        },
        "outputs": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/DataType"
          }
        }
      },
      "required": [
        "node_type",
        "default_overrides",
        "outputs",
        "inputs"
      ]
    },
    "IoType": {
      "type": "string",
      "enum": [
        "File",
        "TcpSocket"
      ]
    },
    "NodeType": {
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "Atomic": {
              "$ref": "#/$defs/AtomicType"
            }
          },
          "additionalProperties": false,
          "required": [
            "Atomic"
          ]
        },
        {
          "type": "object",
          "properties": {
            "Complex": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "required": [
            "Complex"
          ]
        }
      ]
    }
  }
}

